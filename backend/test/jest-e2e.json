{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": "./",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "preset": "ts-jest",
    "moduleDirectories": ["node_modules", "<rootDir>"],
    "moduleNameMapper": {
        "@modules/(.*)": "<rootDir>/../src/modules/$1",
        "@src/(.*)": "<rootDir>../src/$1"
    }
}