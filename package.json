{
    "scripts": {
        "prepare": "husky",
        "be:dev": "npm run --prefix backend dev",
        "be:migrate": "npm run --prefix backend migrate",
        "be:test": "npm run --prefix backend be:test:dev",
        "be:test:e2e": "npm run --prefix backend be:test:e2e",
        "be:test:unit": "npm run --prefix backend test:unit",
        "be:test:fake": "npm run --prefix backend be:test:fake",
        "be:test:all": "npm run be:test && npm run be:test:e2e && npm run be:test:fake",
        "be:lint": "npm run --prefix backend lint",
        "be:lint:fix": "npm run --prefix backend lint:fix",
        "fe:dev": "npm run --prefix frontend fe:dev"
    },
    "devDependencies": {
        "husky": "^9.0.11"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.5",
        "react-redux": "^9.1.2"
    }
}